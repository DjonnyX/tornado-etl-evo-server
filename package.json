    "@types/express-serve-static-core": "^4.17.18",
    "@types/got": "^9.6.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.33",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "nodemon": "^2.0.7"
