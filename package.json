  },
  "homepage": "https://github.com/DjonnyX/tornado-etl-evo-server#readme",
  "license": "Commercial",
  "dependencies": {
    "@djonnyx/tornado-types": "^1.4.72",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^9.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.1.1",
    "tsoa": "^3.8.0",
